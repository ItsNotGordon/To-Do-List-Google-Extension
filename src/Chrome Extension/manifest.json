{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.1",

    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts":
        {
          "matches": ["http://localhost:5173/"],
          "js": ["contentScript.js"]
        },
    "permissions": [
        "storage"
    ],
    "default_locale": "en",
    "description": "A quick to-do list built into Google Chrome",

    "author": "itsnotgordon@gmail.com"
}
